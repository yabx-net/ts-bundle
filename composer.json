{
	"name": "yabx/ts-bundle",
	"type": "symfony-bundle",
	"minimum-stability": "stable",
	"description": "TypeScript mapper for Symfony 6.4",
	"license": "MIT",
	"require": {
		"php": "^8.3",
		"symfony/console": "^6.4",
		"symfony/serializer": "^6.4",
		"symfony/http-kernel": "^6.4",
		"symfony/framework-bundle": "^6.4",
		"yabx/rest-bundle": "^0.4.1"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Yabx\\TypeScriptBundle\\": "src/"
		}
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "6.*.*"
		}
	}
}
