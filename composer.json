{
	"name": "yabx/ts-bundle",
	"version": "0.3.1",
	"type": "symfony-bundle",
	"minimum-stability": "stable",
	"description": "TypeScript mapper for Symfony 6",
	"license": "MIT",
	"require": {
		"php": "^8.2",
		"symfony/console": "^6.2",
		"symfony/serializer": "^6.2",
		"symfony/http-kernel": "^6.2",
		"symfony/framework-bundle": "^6.2",
		"yabx/rest-bundle": "^0.3.1"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Yabx\\TypeScriptBundle\\": "src/"
		}
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "6.*.*"
		}
	},
	"-repositories": [
		{
			"type": "path",
			"url": "../rest-bundle"
		}
	]
}
