{
	"name": "yabx/ts-bundle",
	"type": "symfony-bundle",
	"minimum-stability": "stable",
	"description": "TypeScript mapper for Symfony 7.0",
	"license": "MIT",
	"require": {
		"php": "^8.3",
		"symfony/console": "^7.0|^6.4",
		"symfony/serializer": "^7.0|^6.4",
		"symfony/http-kernel": "^7.0|^6.4",
		"symfony/framework-bundle": "^7.0|^6.4",
		"yabx/rest-bundle": "^0.5"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Yabx\\TypeScriptBundle\\": "src/"
		}
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "7.*.*|6.4.*"
		}
	}
}
